pragma solidity ^0.8.18;

import {Test, console} from "forge-std/Test.sol";
import {DeployKittyMoodNFT} from "../script/DeployKittyMoodNFT.s.sol";
import {KittyMoodNFT} from "../src/KittyMoodNFT.sol";

contract KittyMoodNFTTest is Test {
    KittyMoodNFT kittyMoodNFT;
    string public constant happyCatImageURI = "data:image/svg+xml;base64,";
    string public constant sadCatImageURI = "data:image/svg+xml;base64,";
    string public constant happyCatURI = "data:application/json;base64,";
    string public constant sadCatURI = "data:application/json;base64,";

    address public user = makeAddr("user");
    address public attacker = makeAddr("attacker");
    uint256 public constant START_ETHER = 10 ether;

    // Events
    event mintedNFT(uint256 indexed tokenId, address indexed owner);
    event moodFlipped(uint256 indexed tokenId, KittyMoodNFT.Mood indexed oldMood, KittyMoodNFT.Mood indexed newMood);

    function setUp() external {
        DeployKittyMoodNFT deployKittyMoodNFT = new DeployKittyMoodNFT();
        kittyMoodNFT = deployKittyMoodNFT.run();

        vm.deal(user, START_ETHER);
        vm.deal(attacker, START_ETHER);
    }

    function test_ConstructorSetsUpCorrectly() public {
        assertEq(kittyMoodNFT.getHappyCatImageURI(), happyCatImageURI);
        assertEq(kittyMoodNFT.getSadCatImageURI(), sadCatImageURI);
        assertEq(kittyMoodNFT.getTokenCounter(), 0);
    }

    function test_viewTokenURI() public {
        uint256 tokenId = kittyMoodNFT.getTokenCounter();

        vm.prank(user);
        vm.expectEmit(true, true, false, false, address(kittyMoodNFT));
        emit mintedNFT(tokenId, user);

        kittyMoodNFT.mintKittyNFT();

        assert(kittyMoodNFT.getMood(tokenId) == KittyMoodNFT.Mood.Happy);
        assertEq(kittyMoodNFT.getTokenCounter(), tokenId + 1);
        assertEq(user, kittyMoodNFT.ownerOf(tokenId));
        assertEq(kittyMoodNFT.balanceOf(user), 1);
        assertEq(kittyMoodNFT.tokenURI(tokenId), happyCatURI);

        console.log(kittyMoodNFT.tokenURI(tokenId));
    }

    function test_flipKittyMood_RevertsIfTokenIdNotExist(uint256 randomTokenId) public {
        vm.prank(attacker);
        vm.expectRevert(
            abi.encodeWithSignature("KittyMoodNFT__invalidTokenId()")
        );
        kittyMoodNFT.flipKittyMood(randomTokenId);
    }

    function test_flipKittyMood_RevertsIfCallerIsNotOwnerOfNFT() public {
        uint256 tokenId = kittyMoodNFT.getTokenCounter();

        vm.prank(user);
        kittyMoodNFT.mintKittyNFT();

        vm.prank(attacker);
        vm.expectRevert(
            abi.encodeWithSignature("KittyMoodNFT__notNFTOwner()")
        );
        kittyMoodNFT.flipKittyMood(tokenId);
    }

    function test_flipMoodToSad() public {
        uint256 tokenId = kittyMoodNFT.getTokenCounter();

        vm.prank(user);
        kittyMoodNFT.mintKittyNFT();

        vm.prank(user);
        vm.expectEmit(true, true, true, false, address(kittyMoodNFT));
        emit moodFlipped(tokenId, KittyMoodNFT.Mood.Happy, KittyMoodNFT.Mood.Sad);

        kittyMoodNFT.flipKittyMood(tokenId);

        assert(kittyMoodNFT.getMood(tokenId) == KittyMoodNFT.Mood.Sad);
        assertEq(kittyMoodNFT.tokenURI(tokenId), sadCatURI);

        console.log(kittyMoodNFT.tokenURI(tokenId));
    }
}